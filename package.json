{
  "name": "perfmanx",
  "version": "1.0.0",
  "description": "Perfman with htmx",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "jest",
    "sass": "sass --watch src/sass:public/css",
    "dev": "concurrently \"nodemon src/app.js\" \"sass --watch src/scss:public/css\"",
    "compile-sass": "sass --watch src/scss:public/styles",
    "start": "node src/app.js",
    "postinstall": "cp node_modules/htmx.org/dist/htmx.min.js public/javascripts/vendor/htmx.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alex-Apex/PerfManX.git"
  },
  "keywords": [
    "perfman"
  ],
  "author": "Alex Gomez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Alex-Apex/PerfManX/issues"
  },
  "homepage": "https://github.com/Alex-Apex/PerfManX#readme",
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-handlebars": "^7.1.2",
    "express-session": "^1.17.3",
    "handlebars": "^4.7.8",
    "htmx.org": "^1.9.10",
    "latest": "^0.2.0",
    "mssql": "^10.0.1",
    "node-sass": "^9.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1"
  }
}
